{
    "name": "@flowforge/node-red-dashboard-third-party",
    "version": "0.0.1",
    "description": "A basic example of a third-party node that integrates with Node-RED Dashboard 2.0",
    "keywords": [
        "node-red",
        "node-red-dashboard-2"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/flowforge/node-red-dashboard-3rd-party.git"
    },
    "license": "Apache-2.0",
    "author": {
        "name": "Joe Pavitt",
        "url": "https://github.com/joepavitt"
    },
    "contributors": [
        {
            "name": "Pez Cuckow",
            "url": "https://github.com/pezmc"
        }
    ],
    "exports": {
        "import": "./dist/node-red-dashboard.es.js",
        "require": "./dist/node-red-dashboard.umd.js"
    },
    "files": [
        "dist/*",
        "nodes/*"
    ],
    "scripts": {
        "build": "vite build",
        "dev": "vite",
        "lint": "npm run lint:js && npm run lint:package",
        "lint:fix": "npm run lint:js:fix && npm run lint:package:fix",
        "lint:js": "eslint --ext .js,.vue,.cjs,.mjs .",
        "lint:js:fix": "yarn lint:js --fix",
        "lint:package": "sort-package-json --check 'package.json'",
        "lint:package:fix": "sort-package-json 'package.json'",
        "preview": "vite preview"
    },
    "dependencies": {
        "to-title-case": "^1.0.0",
        "vue": "^3.3.4"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^4.4.1",
        "eslint": "^8.53.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-import": "^2.29.0",
        "eslint-plugin-n": "^16.3.1",
        "eslint-plugin-vue": "^9.18.1",
        "vite": "^4.5.0"
    },
    "engines": {
        "node": ">=14"
    },
    "node-red": {
        "version": ">=3.0.0",
        "nodes": {
            "ui-example": "nodes/ui-example.js"
        }
    },
    "node-red-dashboard-2": {
        "version": "1.0.0",
        "widgets": {
            "ui-example": {
                "source": "ui/UIExample.vue"
            }
        }
    }
}
